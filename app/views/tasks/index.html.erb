<h1>My Tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <li class="li"><%= check_box_tag :completed, "1", task.completed, disabled: true %><%= link_to task.title, task_path(task) %></li>
  <% end %>
</ul>

<button type="button" class="btn btn-primary"><%= link_to "Create new task", new_path(@tasks) %></button>

